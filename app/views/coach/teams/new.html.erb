

<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new team</h1>
  <%= simple_form_for @team, url: coach_teams_path do |f| %>
    <%= f.input :Name %>
    <%= f.input :email %>
    <%= f.input :size %>
    <%= f.input :message %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>